ge.removeItem(\"token\")\n          setProfileState({\n            name: \"\",\n            accountName: \"\",\n            email: \"\",\n            recipes: [],\n            savedRecips: [],\n            token: \"\",\n            id: \"\",\n            isLoggedIn: false\n          })\n        }\n      })\n    }\n  }\n\n  const handleLogOut = event => {\n    let confirmAlert = window.confirm(\"Are you sure to log out?\")\n    if (confirmAlert === true) {\n      localStorage.removeItem(\"token\");\n      setProfileState({\n        name: \"\",\n        email: \"\",\n        recipes: [],\n        token: \"\",\n        isLoggedIn: false\n      })\n      window.location.reload(false)\n    }\n  }\n\n  return (\n    <Router >\n      <NavBar\n        id={profileState.id}\n        accountName={profileState.accountName}\n        isLoggedIn={profileState.isLoggedIn}\n        handleLogOut={handleLogOut}\n      />\n      <Switch>\n        <Route exact path=\"/\">\n          <HomePage />\n        </Route>\n        <Route exact path=\"/signup\">\n          <SignUpPage />\n        </Route>\n        <Route exact path=\"/signin\">\n          <SignInPage />\n        </Route>\n        {/* <Route exact path=\"/profile/:id\"> */}\n        <Route exact path={`/profile/${profileState.accountName}`}>\n          <ProfilePage\n          // profile={profileState}\n          users={usersState}\n          />\n        </Route>\n        <Route exact path=\"/recipeform\">\n          <RecipePage\n            profile={profileState}\n          />\n        </Route>\n        <Route exact path=\"/ingredientform/:id\">\n          <IngrePage\n            profile={profileState}\n          />\n        </Route>\n        <Route exact path=\"/recipe/:id\">\n          <RecipeCardPage\n            profile={profileState}\n          />\n        </Route>\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n","C:\\Users\\JaeShin\\code\\personal\\ummas-cb\\ummacb\\src\\components\\NavBar\\index.js",["95"]